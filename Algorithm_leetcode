# 58
class Solution :
    def lengthOfLastWord(self, s:str) -> int :
        s_split = s.split()
        if len(s_split)==0 :
            return 0
        else :
            return len(s_split[-1])
            
            
class Solution :
    def lengthOfLastWord(self, s:str) -> int :
        s_split = s.split()
        if len(s_split)==0 :
            return 0
            return len(s_split[-1])

